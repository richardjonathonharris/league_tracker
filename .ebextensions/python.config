option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "league.settings"
    PYTHONPATH: "$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: "league/wsgi.py"
    StaticFiles: "/static/=www/static/"
packages: 
  yum:
    postgresql95-devel: []
container_commands:
  01_migrate:
    command: "python manage.py migrate"
    leader_only: true
  02_create_bye:
    command: "python manage.py loaddata bye_data.yaml"
    leader_only: true
  03_collectstatic:
    command: "python manage.py collectstatic --noinput"
  04_createsu:
    command: "python manage.py createsu"
    leader_only: true
